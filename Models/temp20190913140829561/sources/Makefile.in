DLLEXT=@DLLEXT@
NEED_RUNTIME=@NEED_RUNTIME@
NEED_DGESV=@NEED_DGESV@
FMIPLATFORM=@FMIPLATFORM@
# Note: Simulation of the fmu with dymola does not work with -finline-small-functions (enabled by most optimization levels)
CPPFLAGS=@CPPFLAGS@
OMC_NUM_LINEAR_SYSTEMS=2
OMC_NUM_NONLINEAR_SYSTEMS=0
OMC_NUM_MIXED_SYSTEMS=0
OMC_NDELAY_EXPRESSIONS=0
OMC_NVAR_STRING=0

override CPPFLAGS += -Iinclude/ -Iinclude/fmi2 -I.    -DOMC_MODEL_PREFIX=InvertedPendulum -DOMC_NUM_MIXED_SYSTEMS=0 -DOMC_NUM_LINEAR_SYSTEMS=2 -DOMC_NUM_NONLINEAR_SYSTEMS=0 -DOMC_NDELAY_EXPRESSIONS=0 -DOMC_NVAR_STRING=0

MAINFILE=InvertedPendulum_FMU.c
MAINOBJ=InvertedPendulum_FMU.o
CFILES=InvertedPendulum.c InvertedPendulum_functions.c InvertedPendulum_records.c \
InvertedPendulum_01exo.c InvertedPendulum_02nls.c InvertedPendulum_03lsy.c InvertedPendulum_04set.c InvertedPendulum_05evt.c InvertedPendulum_06inz.c InvertedPendulum_07dly.c \
InvertedPendulum_08bnd.c InvertedPendulum_09alg.c InvertedPendulum_10asr.c InvertedPendulum_11mix.c InvertedPendulum_12jac.c InvertedPendulum_13opt.c InvertedPendulum_14lnz.c \
InvertedPendulum_15syn.c InvertedPendulum_16dae.c InvertedPendulum_17inl.c InvertedPendulum_init_fmu.c
OFILES=$(CFILES:.c=.o)
GENERATEDFILES=$(MAINFILE) InvertedPendulum_FMU.makefile InvertedPendulum_literals.h InvertedPendulum_model.h InvertedPendulum_includes.h InvertedPendulum_functions.h  InvertedPendulum_11mix.h InvertedPendulum_12jac.h InvertedPendulum_13opt.h InvertedPendulum_init_fmu.c InvertedPendulum_info.c $(CFILES) InvertedPendulum_FMU.libs

# FIXME: before you push into master...
RUNTIMEDIR=include
OMC_MINIMAL_RUNTIME=1
OMC_FMI_RUNTIME=1
include $(RUNTIMEDIR)/Makefile.objs
ifneq ($(NEED_RUNTIME),)
RUNTIMEFILES=$(FMI_ME_OBJS:%=$(RUNTIMEDIR)/%.o)
endif

PHONY: InvertedPendulum_FMU
InvertedPendulum_FMU: nozip
	cd .. && rm -f ../InvertedPendulum.fmu && zip -r ../InvertedPendulum.fmu *
nozip: $(MAINOBJ) InvertedPendulum_functions.h InvertedPendulum_literals.h $(OFILES) $(RUNTIMEFILES)
	mkdir -p ../binaries/$(FMIPLATFORM)
ifeq (@LIBTYPE_DYNAMIC@,1)
	$(LD) -o InvertedPendulum$(DLLEXT) $(MAINOBJ) $(OFILES) $(RUNTIMEFILES) -L"/home/lapbottom/Programming/Exjobb/Models"   $(LDFLAGS)
	cp InvertedPendulum$(DLLEXT) InvertedPendulum_FMU.libs ../binaries/$(FMIPLATFORM)/
endif
	head -n20 Makefile > ../resources/$(FMIPLATFORM).summary
ifeq (@LIBTYPE_STATIC@,1)
	rm -f InvertedPendulum.a
	$(AR) -rsu InvertedPendulum.a $(MAINOBJ) $(OFILES) $(RUNTIMEFILES)
	cp InvertedPendulum.a InvertedPendulum_FMU.libs ../binaries/$(FMIPLATFORM)/
endif
	$(MAKE) distclean
distclean: clean
	rm -f Makefile config.status config.log
clean:
	rm -f InvertedPendulum.def InvertedPendulum.o InvertedPendulum.a InvertedPendulum$(DLLEXT) $(MAINOBJ) $(OFILES) $(RUNTIMEFILES)